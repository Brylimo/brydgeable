mixin items(item)
	a(href=`/item/${item.id}`).video-mixin
		div.video-mixin__thumb(style=`background-image:url(${item.thumbUrl}); background-size:cover; background-position:center;`)
		div.video-mixin__data
			span.video-mixin__title=item.title 
			div.video-mixin__meta
				span #{item.owner.username} • #{item.views}회